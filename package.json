{
    "name": "kirsty-codes",
    "version": "1.0.0",
    "description": "A personal blog, updated sporadically",
    "main": ".eleventy.js",
    "scripts": {
        "start": "npm-run-all build:sass --parallel watch:*",
        "build:sass": "sass src/sass:dist/css",
        "build:eleventy": "eleventy",
        "watch:sass": "sass --watch src/sass:dist/css",
        "watch:eleventy": "eleventy --serve",
        "clean": "rm -rf dist/",
        "build": "run-s -l build:eleventy",
        "format": "prettier --write '**/*.{js,css,html,md}'"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "path": "^0.12.7",
        "sass": "^1.56.1"
    },
    "devDependencies": {
        "@11ty/eleventy": "^2.0.0",
        "@11ty/eleventy-img": "^3.1.8",
        "@11ty/eleventy-plugin-rss": "^1.2.0",
        "npm-run-all": "^4.1.5",
        "prettier": "3.2.1"
    },
    "volta": {
        "node": "16.18.1"
    }
}
